type Dog {
  _id: String!
  name: String!
  isGoodBoi: Boolean!
}

type Query {
  dog(_id: String!): Dog!
  dogs: [Dog!]!
}

input NewDog {
  name: String!
  isGoodBoi: Boolean!
}

type Mutation {
  createDog(input: NewDog): Dog!
  # La ling pour ajouter plusieurs dogs en mÃªme temp 
  # createDogs(inputs: [CreateDogInput!]!): [Dog!]! 
}
